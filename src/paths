# didn't use AI here :D
# I usually love rush coding with AI
# but gotta stay sharp :)

paths() {
if [[ -z $1 ]];then
    echo "Usage: paths <file> <int> [-f]"
    return 1
fi
local inj
for i in $(seq 1 $2);
do
    inj+="\$$((i+3))\"/\""
done
if [[ "$arg" == "-f" ]];then
grep http $1 | awk -F / '{print $1"//"$3"/"'$inj'}' | grep "\." | sort -u
else
grep http $1 | awk -F / '{print $1"//"$3"/"'$inj'}' | grep -v "\." | sort -u
fi
}
